{
  "name": "song-creation-pipeline",
  "version": "0.0.1",
  "description": "Collection of Scripts used to help generate music mostly automatically",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "get-samples": "node src/get-samples.js guitar+electro",
    "get-word-suggestions": "node src/get-word-suggestions.js Gitarre"
  },
  "keywords": [
    "ai",
    "music"
  ],
  "author": "Thomas Maier",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/local-auth": "3.0.0",
    "axios": "1.6.1",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "googleapis": "105"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.10.4",
    "concurrently": "8.2.2",
    "typescript": "5.3.3",
    "nodemon": "3.0.2"
  }
}
