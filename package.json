{
  "name": "workstation",
  "version": "0.2.3",
  "description": "Express App Server to help improve daily life",
  "main": "src/index.ts",
  "scripts": {
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "test": "bash test-runner.sh",
    "test:ci": "node --test dist/tests/*.test.js",
    "test:coverage": "node --test --experimental-test-coverage dist/tests/*.test.js",
    "build": "tsc",
    "start": "node dist/index.js",
    "image:build": "docker buildx build --platform linux/arm/v7 -t workstation ."
  },
  "keywords": [
    "apple reminders",
    "planner",
    "generator",
    "music"
  ],
  "author": "Thomas Maier",
  "license": "ISC",
  "dependencies": {
    "axios": "1.6.7",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "16.4.1",
    "express": "4.18.2",
    "googleapis": "131.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.11.7",
    "concurrently": "8.2.2",
    "nodemon": "3.0.3",
    "typescript": "5.3.3"
  }
}
